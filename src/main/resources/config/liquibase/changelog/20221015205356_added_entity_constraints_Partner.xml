<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Partner.
    -->
    <changeSet id="20221015205356-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="card_id"
                                 baseTableName="partner"
                                 constraintName="fk_partner__card_id"
                                 referencedColumnNames="id"
                                 referencedTableName="card"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="partner"
                                 constraintName="fk_partner__company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>
    </changeSet>
</databaseChangeLog>
